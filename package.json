{
  "name": "eos-console",
  "version": "0.0.0",
  "description": "Library to interface with ETC Eos Family lighting consoles.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "demo": "ts-node --files demo.ts",
    "format": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "mocha -r ts-node/register src/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglasfinlay/node-eos-console.git"
  },
  "keywords": [
    "etc",
    "eos",
    "lighting",
    "osc",
    "theatre"
  ],
  "author": "Douglas Finlay <douglas@douglasfinlay.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/douglasfinlay/node-eos-console/issues"
  },
  "homepage": "https://github.com/douglasfinlay/node-eos-console#readme",
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.2",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chai": "^4.3.10",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "osc-min": "^1.1.2",
    "slip": "^1.0.2"
  }
}
